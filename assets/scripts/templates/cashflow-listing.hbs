<h1>BUDGET ITEM LIST</h1>
{{#each cashflows as |cashflow|}}

  <ul data-id={{cashflow.id}}></ul>
  <li>
    {{cashflow.id}}
    NAME= {{cashflow.name}}
    VALUE= {{cashflow.value}}
  </li>
  <form>
    <fieldset class='delete-cashflow'>
      <input type="submit" name="submit" value="delete">
      <input type="number" name='cashflow[id]' class="input" value={{cashflow.id}}>
    </fieldset>
  </form>
{{/each}}
